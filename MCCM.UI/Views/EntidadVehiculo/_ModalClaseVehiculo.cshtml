@model MCCM.Entidad.TMCCM_C_Vehiculo_Clase
    <div class="modal fade" id="modal_C_ClaseVehiculo" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title col-11 text-center">Insertar Clase</h5>
                    <button type="button" class="close col-1" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form id="Form_C_ClaseVehiculo">
                        <div class="form-group row justify-content-center">
                            <label for="descripcion" class="col-md-3 col-form-label">Descripcion</label>
                            <input class="col-md-8 form-control" name="TC_Descripcion" id="TC_Descripcion_C_ClaseVehiculo">
                        </div>
                        <div class="form-group row justify-content-center">
                            <button id="btnAgregar_C_ClaseVehiculo" type="submit" class="btn btn-primary submit-button mx-2">Insertar</button>


                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>
