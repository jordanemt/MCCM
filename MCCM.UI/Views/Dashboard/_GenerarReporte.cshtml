<div id="generarReporte-form" class="container col-12">
    <div class="row border-light mb-3   ">


        <h6 class="badge badge-primary" style="margin:1.0%;">
            Nombre del Caso:
        </h6>
        <input type="text" class="col-3 form-control" style="margin-left: 2%!important; margin-top:0.5%!important;">


        <button class="btn badge-primary  col-sm-3" style="margin-left: 60%;
        margin-top: -3.8% !important;" data-toggle="popover" data-content='<input type=' id='picker'>
            Rango de Fechas
        </button>

    </div>


    <div class="row ">
        <div class="card align-content-center col-12 p-0" style="border-color:#FFB140">
            <div class="card-header amarillo">
                Reporte
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" value="" id="BtnSeleccionar">
                    <label class="form-check-label" for="defaultCheck1">
                        Marcar Todo
                    </label>
                </div>
            </div>

            <div class="card-body">
                <div class="container w-100">
                    <div class="row">
                        <div class="card col-4 p-0">
                            <div class="card-header align-content-center col-16 gris_claro">
                                Bitácora
                            </div>
                            <div class="card-body" id="bitacora">

                                <div class="col-sm-12">
                                    <div class="row">

                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="Eventos">

                                            <span class="badge badge-primary" for="defaultCheck1">
                                                Eventos
                                            </span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="TareasTerminadas">

                                            <span class="badge badge-primary" for="defaultCheck1">
                                                Tareas Terminadas
                                            </span>
                                        </div>

                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="TareasPendientes">
                                            <span class="badge badge-primary" disabled for="defaultCheck1">
                                                Tareas Pendientes
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="card col-4 p-0">
                            <div class="card-header align-content-center col-16 gris_claro">
                                Entidades
                            </div>
                            <div class="card-body" id="entidades">

                                <div class="col-sm-12">
                                    <div class="row">

                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="Persona">

                                            <label class="fa fa-user" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>
                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="PersonaJuridica">

                                            <label class="fa fa-building font_negro" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>
                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="EntidadVehiculo">

                                            <label class="fa fa-car" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>

                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="Ubicacion">

                                            <label class="fa fa-map-marker" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>
                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="Telefono">

                                            <label class="fa fa-phone" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>

                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="Arma">

                                            <label class="fa fa-bomb" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>
                                        <div class="col-3">
                                            <input class="form-check-input" type="checkbox" value="" id="Droga">

                                            <label class="fa fa-magic" for="defaultCheck1" style="margin:auto; ">

                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card col-4 p-0">
                            <div class="card-header align-content-center col-16 gris_claro">
                                Recursos
                            </div>
                            <div class="card-body" id="recursos" style="margin:0.5%;">

                                <div class="col-sm-12">
                                    <div class="row">
                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="Personal">

                                            <span class="badge badge-primary" for="defaultCheck1">
                                                Personal Policial
                                            </span>
                                        </div>

                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="Vehiculo">

                                            <span class="badge badge-primary" for="defaultCheck1">
                                                Vehiculos
                                            </span>
                                        </div>
                                    </div>
                                    <div class="row">

                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="Operativo">

                                            <span class="badge badge-primary" for="defaultCheck1">
                                                Gastos Operativos
                                            </span>
                                        </div>

                                        <div class="col-6">
                                            <input class="form-check-input" type="checkbox" value="" id="Combustible">

                                            <span class="badge badge-primary" for="defaultCheck1">
                                                Gastos en Combustible
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
        <div><button type="button" class="btn btn-primary " style="margin-top:10.0%" ; opacity:1" onclick="generarReporte()">Generar Reporte</button></div>
    </div>
</div>