@model MCCM.Entidad.TMCCM_Entidad_Persona_Juridica
<div class="modal fade bd-example-modal-lg" id="entidadPersonaJuridicaModal" tabindex="-1" role="dialog" aria-hidden="true">
<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title col-11 text-center" id="exampleModalLabel">Insertar Entidad Persona Juridica</h5>
            <button type="button" class="close col-1" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form>
                <div class="form-group row">
                    <label for="cedulaJuridica" class="col-md-3 col-form-label">Cedula Juridica</label>
                    <input type="text" class="col-md-8 form-control" name="TN_ID_Cedula_Juridica" id="nombreEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="nombreOrganizacion" class="col-md-3 col-form-label">Nombre Organización</label>
                    <input type="text" class="col-md-8 form-control" name="TC_Nombre_Organización" id="primerApellidoEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="nombreComercial" class="col-md-3 col-form-label">Nombre Comercial</label>
                    <input type="text" class="col-md-8 form-control" name="TC_Nombre_Comercial" id="segundoApellidoEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="icono" class="col-md-3 col-form-label">Icono</label>
                    <select class=" col-md-8 form-control" name="TN_ID_Icono_Persona" id="exampleFormControlSelect1">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                    </select>
                    <a href="#" class="fa fa-plus col-md-1"></a>
                </div>
                <div class="form-group row">
                    <label for="fotografía" class="col-md-3 col-form-label">Fotografía</label>
                    <input type="file" class="col-md-8 form-control-file" name="TB_Fotografia" id="exampleFormControlFile1">
                </div>
                <div class="form-group row">
                    <label for="tipoOrganizacion" class="col-md-3 col-form-label">Tipo Organización</label>
                    <select class=" col-md-8 form-control"  name="TN_ID_Tipo_Organización" id="exampleFormControlSelect1">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                    </select>
                    <a href="#" class="fa fa-plus col-md-1"></a>
                </div>
                <div class="form-group row">
                    <label for="sitioWeb" class="col-md-3 col-form-label">Sitio web</label>
                    <input type="text" class="col-md-8 form-control" name="TC_Sitio_Web" id="identificacionEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="comentario" class="col-md-3 col-form-label">Comentario</label>
                    <textarea class="col-md-8 form-control" name="TC_Comentario" id="exampleFormControlTextarea1" rows="3"></textarea>
                </div>
                <div class="form-group row">
                    <label for="segundoApellido" class="col-md-3 col-form-label">Fecha Modificación</label>
                    <input type="text" class="col-md-8 form-control"name="TF_Fecha_Modificacion" id="segundoApellidoEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="alias" class="col-md-3 col-form-label">Fecha Creacion</label>
                    <input type="text" class="col-md-8 form-control" name="TF_Fecha_Creacion" id="edadEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="creadoPor" class="col-md-3 col-form-label">Creado por</label>
                    <input type="text" class="col-md-8 form-control" name="TC_Creado_Por" id="edadEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="creadoPor" class="col-md-3 col-form-label">Modificado por</label>
                    <input type="text" class="col-md-8 form-control" name="TF_Modificado_Por" id="edadEntidadPersona">
                </div>
                <div class="form-group row">
                    <label for="verificado" class="col-md-3 col-form-label">Verificado</label>
                    <div class="col-md-8  form-check">
                        <input class="form-check-input" type="checkbox" name="TB_Verificado" id="gridCheck1">
                    </div>
                </div>
            </form>
        </div>
        <div class="modal-footer d-flex justify-content-center">
            <button type="button" class="btn btn-primary">Insertar</button>
            <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>

        </div>
    </div>
</div>
</div>

