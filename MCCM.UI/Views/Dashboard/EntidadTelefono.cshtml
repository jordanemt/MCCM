@model MCCM.Entidad.TMCCM_Entidad_Telefono
    <div class="modal fade bd-example-modal-lg" id="entidadTelefonoModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title col-11 text-center" id="exampleModalLabel">Insertar Entidad Telefono</h5>
                    <button type="button" class="close col-1" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group row">
                            <label for="telefono" class="col-md-3 col-form-label">Telefono</label>
                            <input type="text" class="col-md-8 form-control"name="TN_Numero"  id="segundoApellidoEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="empresaTelefonica" class="col-md-3 col-form-label">Proveedor</label>
                            <select class=" col-md-8 form-control"name="TN_ID_Proveedor" id="exampleFormControlSelect1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <a href="#" class="fa fa-plus col-md-1"></a>
                        </div>
                        <div class="form-group row">
                            <label for="iconoTelefono" class="col-md-3 col-form-label">Icono telefono</label>
                            <select class=" col-md-8 form-control" name="TN_ID_Icono_Telefono" id="exampleFormControlSelect1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <a href="#" class="fa fa-plus col-md-1"></a>
                        </div>
                        <div class="form-group row">
                            <label for="ccomentario" class="col-md-3 col-form-label">Comentario</label>
                            <textarea class="col-md-8 form-control" name="TC_Comentario" id="exampleFormControlTextarea1" rows="3"></textarea>
                        </div>
                        <div class="form-group row">
                            <label for="alias" class="col-md-3 col-form-label">Fecha Creacion</label>
                            <input type="text" class="col-md-8 form-control" name="TF_Fecha_Creacion" id="edadEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="creadoPor" class="col-md-3 col-form-label">Creado por</label>
                            <input type="text" class="col-md-8 form-control" name="TC_Creado_Por" id="edadEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="creadoPor" class="col-md-3 col-form-label">Modificado por</label>
                            <input type="text" class="col-md-8 form-control"name="TF_Modificado_Por" id="edadEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="verificado" class="col-md-3 col-form-label">Verificado</label>
                            <div class="col-md-8  form-check">
                                <input class="form-check-input" type="checkbox"name="TB_Verificado" id="gridCheck1">
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer d-flex justify-content-center">
                    <button type="button" class="btn btn-primary">Insertar</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>

                </div>
            </div>
        </div>
    </div>
