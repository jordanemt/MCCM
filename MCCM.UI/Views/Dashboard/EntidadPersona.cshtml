@model MCCM.Entidad.TMCCM_Entidad_Persona
<div class="modal fade bd-example-modal-lg " id="entidadPersonaModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title col-11 text-center" id="exampleModalLabel">Insertar Entidad Persona</h5>
                <h5 class="modal-title col-11 text-center" id="exampleModalLabel" style="display:none">Modificar Entidad Persona</h5>
                <button type="button" class="close col-1" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group row">
                        <label for="nombre" class="col-md-3 col-form-label">Nombre</label>
                        <input type="text" class="col-md-8 form-control" name="TC_Nombre" id="nombreEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="primerApellido" class="col-md-3 col-form-label">Primer apellido</label>
                        <input type="text" class="col-md-8 form-control" name="TC_Primer_Apellido" id="primerApellidoEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="segundoApellido" class="col-md-3 col-form-label">Segundo apellido</label>
                        <input type="text" class="col-md-8 form-control" name="TC_Segundo_Apellido" id="segundoApellidoEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="segundoApellido" class="col-md-3 col-form-label">Fecha Nacimiento</label>
                        <input type="text" class="col-md-8 form-control" name="TF_Fecha_Nacimiento" id="segundoApellidoEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="tipoIdentificacion" class="col-md-3 col-form-label">Tipo identificación</label>
                        <select class=" col-md-8 form-control form-control" name="TN_ID_Tipo_Identificacion" id="exampleFormControlSelect1">
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                        </select>
                        <a href="#" class="fa fa-plus col-md-1"></a>
                    </div>
                    <div class="form-group row">
                        <label for="identificacion" class="col-md-3 col-form-label">Identificación</label>
                        <input type="text" class="col-md-8 form-control" name="TC_Cedula" id="identificacionEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="edad" class="col-md-3 col-form-label">Edad</label>
                        <input type="number" class="col-md-8 form-control" name="TN_Edad" id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="iconoPersona" class="col-md-3 col-form-label">Icono persona</label>
                        <select class=" col-md-8 form-control" name="TN_ID_Icono_Persona" id="exampleFormControlSelect1">
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                        </select>
                        <a href="#" class="fa fa-plus col-md-1"></a>
                    </div>
                    <div class="form-group row">
                        <label for="fotografía" class="col-md-3 col-form-label">Fotografía</label>
                        <input type="file" class="col-md-8 form-control-file" name="TB_Fotografía" id="exampleFormControlFile1">
                    </div>
                    <div class="form-group row">
                        <label for="sexo" class="col-md-3 col-form-label">Sexo</label>
                        <select class=" col-md-8 form-control" name="TN_ID_Sexo" id="exampleFormControlSelect1">
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                        </select>
                        <a href="#" class="fa fa-plus col-md-1"></a>
                    </div>
                    <div class="form-group row">
                        <label for="genero" class="col-md-3 col-form-label">Genero</label>
                        <select class=" col-md-8 form-control" name="TN_ID_Genero" id="exampleFormControlSelect1">
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                        </select>
                    </div>
                    <div class="form-group row">
                        <label for="nacionalidad" class="col-md-3 col-form-label">Nacionalidad</label>
                        <select class=" col-md-8 form-control" name="TN_ID_Nacionalidad" id="exampleFormControlSelect1">
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                        </select>
                        <a href="#" class="fa fa-plus col-md-1"></a>
                    </div>
                    <div class="form-group row">
                        <label for="fallecido" class="col-md-3 col-form-label">Fallecido</label>
                        <div class="col-md-8  form-check">
                            <input class="form-check-input" type="checkbox" name="TB_Fallecido" id="gridCheck1">

                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="nAutopsia" class="col-md-3 col-form-label">N° Autopsia</label>
                        <input type="text" class="col-md-8 form-control" name="TN_Autopsia"  id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="expCriminal" class="col-md-3 col-form-label">Expediente Criminal</label>
                        <div class="col-md-8  form-check">
                            <input class="form-check-input" type="checkbox" name="TB_Exp_Criminal" id="gridCheck1">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="alias" class="col-md-3 col-form-label">Alias</label>
                        <input type="text" class="col-md-8 form-control" name="TC_Alias" id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="segundoApellido" class="col-md-3 col-form-label">Fecha Modificación</label>
                        <input type="text" class="col-md-8 form-control" name="TF_Fecha_Modificacion" id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="alias" class="col-md-3 col-form-label">Fecha Creacion</label>
                        <input type="text" class="col-md-8 form-control" name="TF_Fecha_Creacion"  id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="creadoPor" class="col-md-3 col-form-label">Creado por</label>
                        <input type="text" class="col-md-8 form-control" name="TC_Creado_Por" id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="creadoPor" class="col-md-3 col-form-label">Modificado por</label>
                        <input type="text" class="col-md-8 form-control" name="TF_Modificado_Por" id="edadEntidadPersona">
                    </div>
                    <div class="form-group row">
                        <label for="verificado" class="col-md-3 col-form-label">Verificado</label>
                        <div class="col-md-8  form-check">
                            <input class="form-check-input" type="checkbox" name="TB_Verificado" id="gridCheck1">
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer d-flex justify-content-center">
                <button type="button" class="btn btn-primary">Insertar</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>

            </div>
        </div>
    </div>
</div>

