@model MCCM.Entidad.TMCCM_Entidad_Vehiculo
    <div class="modal fade bd-example-modal-lg" id="entidadVehículoModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title col-11 text-center" id="exampleModalLabel">Insertar Entidad Vehículo</h5>
                    <button type="button" class="close col-1" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group row">
                            <label for="placa" class="col-md-3 col-form-label">Placa</label>
                            <input type="text" class="col-md-8 form-control" name="TC_Placa" id="nombreEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="marca" class="col-md-3 col-form-label">Marca</label>
                            <select class=" col-md-8 form-control" name="TN_ID_Marca_Vehiculo" id="exampleFormControlSelect1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <a href="#" class="fa fa-plus col-md-1"></a>
                        </div>
                        <div class="form-group row">
                            <label for="clase" class="col-md-3 col-form-label">Clase</label>
                            <select class=" col-md-8 form-control"  name="TC_Clase" id="exampleFormControlSelect1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <a href="#" class="fa fa-plus col-md-1"></a>
                        </div>
                        <div class="form-group row">
                            <label for="fotografía" class="col-md-3 col-form-label">Fotografía</label>
                            <input type="file" class="col-md-8 form-control-file"  name="TB_Fotografia" id="exampleFormControlFile1">
                        </div>
                        <div class="form-group row">
                            <label for="color" class="col-md-3 col-form-label">Color</label>
                            <select class=" col-md-8 form-control"name="TN_ID_Color_Vehiculo" id="exampleFormControlSelect1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <a href="#" class="fa fa-plus col-md-1"></a>
                        </div>
                        <div class="form-group row">
                            <label for="estilo" class="col-md-3 col-form-label">Estilo</label>
                            <input type="text" class="col-md-8 form-control" name="TC_Estilo" id="segundoApellidoEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="iconoVehiculo" class="col-md-3 col-form-label">Icono Vehículo</label>
                            <select class=" col-md-8 form-control" name="TN_ID_Icono_Vehiculo" id="exampleFormControlSelect1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <a href="#" class="fa fa-plus col-md-1"></a>
                        </div>
                        <div class="form-group row">
                            <label for="año" class="col-md-3 col-form-label">Año</label>
                            <input type="number" class="col-md-8 form-control" name="TN_Anno" id="identificacionEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="comentario" class="col-md-3 col-form-label">Comentario</label>
                            <textarea class="col-md-8 form-control" name="TC_Comentario" id="exampleFormControlTextarea1" rows="3"></textarea>
                        </div>
                        <div class="form-group row">
                            <label for="segundoApellido" class="col-md-3 col-form-label">Fecha Modificación</label>
                            <input type="text" class="col-md-8 form-control" name="TF_Fecha_Modificacion" id="segundoApellidoEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="alias" class="col-md-3 col-form-label">Fecha Creacion</label>
                            <input type="text" class="col-md-8 form-control" name="TF_Fecha_Creacion" id="edadEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="creadoPor" class="col-md-3 col-form-label">Creado por</label>
                            <input type="text" class="col-md-8 form-control" name="TC_Creado_Por" id="edadEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="creadoPor" class="col-md-3 col-form-label">Modificado por</label>
                            <input type="text" class="col-md-8 form-control" name="TF_Modificado_Por" id="edadEntidadPersona">
                        </div>
                        <div class="form-group row">
                            <label for="verificado" class="col-md-3 col-form-label">Verificado</label>
                            <div class="col-md-8  form-check">
                                <input class="form-check-input" type="checkbox" name="TB_Verificado" id="gridCheck1">
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer d-flex justify-content-center">
                    <button type="button" class="btn btn-primary">Insertar</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>

                </div>
            </div>
        </div>
    </div>
